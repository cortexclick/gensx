{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "bundler",
    "allowImportingTsExtensions": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "react-jsx",
    "jsxImportSource": "gensx",
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noFallthroughCasesInSwitch": true,
    "skipLibCheck": true,
    "noEmit": true,
    "baseUrl": ".",
    "paths": {
      "gensx": [
        "../../packages/gensx/src"
      ],
      "gensx/*": [
        "../../packages/gensx/src/*"
      ],
      "@gensx/openai": [
        "../../packages/gensx-openai/src"
      ],
      "@gensx/openai/*": [
        "../../packages/gensx-openai/src/*"
      ]
    }
  }
}